import java.util.Hashtable;
import java.util.Set;

// Determine if a string has all unique characters
public class isUnique {
	public static void main(String[] args) {
		System.out.println(hasAllUnique2("run"));
		System.out.println();
		System.out.println(hasAllUnique2("Project"));
		System.out.println();
		System.out.println(hasAllUnique2("faiza"));
		System.out.println();
		System.out.println(hasAllUnique2("Search"));
		System.out.println();
		System.out.println(hasAllUnique2("help"));
		System.out.println();
		System.out.println(hasAllUnique2("ahsan"));
	}
	
	public static Boolean hasAllUnique(String str) {
		Boolean isUnique = true;
		
		char[] chars = str.toCharArray();
		Hashtable<Character, Integer> hash = new Hashtable<Character, Integer>();
		
		for(int i = 0; i <= chars.length-1; i++) {
			if(hash.contains(chars[i])) {
				chars[i] += 1;
			} else {
				hash.put(chars[i], 1);
			}
		}
		System.out.println(chars); 
		System.out.println("Hash: " + hash); 
		
		if (chars.length != hash.size()) {
			isUnique = false;
		}
		return isUnique;
	}
	
	public static Boolean hasAllUnique2(String str) {
		Boolean isUnique = true;
		int count = 0;
		
		for (int i = 0; i <= str.length() - 1; i++) {
			for(int j = i + 1; j <= str.length(); j++) {
				if(str.charAt(i) == str.charAt(j)) {
					return false; 
				}
			}
		}
		return isUnique;
	}
}
